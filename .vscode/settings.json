{
    "eslint.enable": true,
    "editor.formatOnSave": true,
    "editor.formatOnType": true,
    "[javascript]": {
      "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[typescript]": {
      "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[typescriptreact]": {
      "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[json]": {
      "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[yaml]": {
      "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[html]": {
      "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "files.eol": "\n",
    "editor.detectIndentation": false,
    "editor.tabSize": 2,
    "npm.packageManager": "yarn",
    "typescript.tsdk": "node_modules\\typescript\\lib"
  }
  