    "babel-jest": "^26.0.1",
    "cypress": "^4.8.0",
    "jest": "^26.0.1",
    "parcel-bundler": "^1.12.4",
    "prettier": "^2.0.5",
    "typescript": "^3.9.5"
  },
  "dependencies": {
    "@types/d3": "^5.7.2",
    "d3": "^5.16.0",
    "whatwg-fetch": "^3.0.0"
